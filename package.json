{
  "name": "geo-view",
  "version": "0.1.0-alpha.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --browsers ChromeHeadless --watch=false --code-coverage",
    "test:coverage": "ng test --browsers ChromeHeadless --code-coverage",
    "test:1": "ng test --include src/app/path-to-your-layer-service/layer.service.spec.ts --browsers ChromeHeadless --watch=false\n",
    "test:2": "ng test --include src/app/services/layer.service.spec.ts --browsers ChromeHeadless --watch=true",
    "release": "npm version patch && git push && git push origin --tags && npm publish dist/ixtlan"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 80
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@dvanauken/ixtlan": "~0.1.13-alpha",
    "d3": "^7.9.0",
    "d3-geo": "^3.1.0",
    "lodash": "^4.17.21",
    "papaparse": "^5.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.15",
    "@angular/cli": "^16.2.15",
    "@angular/compiler-cli": "^16.2.0",
    "@types/d3": "^7.4.3",
    "@types/geojson": "^7946.0.14",
    "@types/jasmine": "~4.3.0",
    "@types/leaflet": "^1.9.3",
    "@types/lodash": "^4.17.12",
    "@types/papaparse": "^5.3.15",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.3.3",
    "typescript": "~5.1.3"
  }
}
